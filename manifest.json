{
  "manifest_version": 2,

  "name": "Math Equations Anywhere",
  "description": "This extension allows you to create math equation with latex, mathml, ascii text and convert them to images.",
  "version": "1.1",
  "icons": { "16": "./images/16x16.png",
             "24": "./images/24x24.png",
             "32": "./images/32x32.png",
             "48": "./images/48x48.png",
             "64": "./images/64x64.png",
             "96": "./images/96x96.png",
             "128": "./images/128x128.png" 
  },
  "browser_action": {
    "default_icon": {
      "16": "./images/16x16.png",
      "24": "./images/24x24.png",
      "32": "./images/32x32.png",
      "48": "./images/48x48.png",
      "64": "./images/64x64.png",
      "96": "./images/96x96.png",
      "128": "./images/128x128.png" 
    },
    "default_title": "Math Equations",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "developer": {
    "name": "Brenden A",
    "url": "https://github.com/brendena"
  },
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg",
    "MathJaxEvent.js",
    "StyleSheets/MyCss.css",
    "main.js",
    "clipboard.min.js",
    "ports.js",
    "bower_components/*"
  ],
  
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
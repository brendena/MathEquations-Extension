{
  "manifest_version": 2,

  "name": "Math Equations Anywhere",
  "description": "This extension allows you to create math equation with latex, mathml, ascii text and convert them to Img.",
  "version": "1.1",
  "icons": { "16": "./Img/16x16.png",
             "24": "./Img/24x24.png",
             "32": "./Img/32x32.png",
             "48": "./Img/48x48.png",
             "64": "./Img/64x64.png",
             "96": "./Img/96x96.png",
             "128": "./Img/128x128.png" 
  },
  "browser_action": {
    "default_icon": {
      "16": "./Img/16x16.png",
      "24": "./Img/24x24.png",
      "32": "./Img/32x32.png",
      "48": "./Img/48x48.png",
      "64": "./Img/64x64.png",
      "96": "./Img/96x96.png",
      "128": "./Img/128x128.png" 
    },
    "default_title": "Math Equations",
    "default_popup": "popUp.html"
  },
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js": ["contentScripts.js"],
      "run_at": "document_end"
    }
  ],
  "developer": {
    "name": "Brenden A",
    "url": "https://github.com/brendena"
  },
  "web_accessible_resources": [
    "Img/*.png",
    "Img/*.svg",
    "MathJaxEvent.js",
    "StyleSheets/MyCss.css",
    "main.js",
    "clipboard.min.js",
    "ports.js",
    "bower_components/*",
    "mathEquationComponent.js"
  ],
  
  "permissions": [
    "tabs",
    "activeTab"
  ]
}